<!DOCTYPE html>
<html>
<head>
  <title>Dvrt</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= javascript_include_tag 'https://use.fontawesome.com/92d99cf3de.js' %>
  <%= csrf_meta_tags %>
</head>
<body class="no-sidebar">
  <div id="page-wrapper">

    <!-- Header Wrapper -->
    <div id="header-wrapper">

      <!-- Header -->
      <div id="header">

        <!-- Nav -->
        <nav id="nav">
          <ul>
            <li><%= link_to "Home", root_path %></li>
            <li>
              <a href="#">Dashboard</a>
              <ul>
                <li><%= link_to "Page 1", one_index_path %></li>
                <li><%= link_to "Page 2", two_index_path %></li>
              </ul>
            </li>
          </ul>
        </nav>

      </div>

    </div>

    <!-- Main Wrapper -->
    <div id="main-wrapper">

      <!-- Main -->
      <div id="page" class="container">

        <!-- Main Heading -->
        <div class="title-heading">
          <div class="1u -10u">
            <%= image_tag "divert.jpg", class: 'image' %>
          </div>
        </div>

        <%= yield %>


      </div>

    </body>
    </html>
